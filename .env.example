# ============================================================
# HQX-CLI Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit the .env file!
# ============================================================

# --- Proxy (SOCKS5 residential) ---
# Format: host:port:username:password
HQX_PROXY_FR=
HQX_PROXY_US=
HQX_PROXY_UK=
# Or direct URL: socks5://user:pass@host:port
HQX_PROXY_URL=
# Set to 1 to disable proxy
HQX_PROXY_DISABLED=0
# Region: FRANCE, US, UK
HQX_PROXY_REGION=FRANCE

# --- HQX Server ---
HQX_HOST=
HQX_PORT=3500
HQX_WS_SECURE=true

# --- Debug ---
HQX_DEBUG=0
